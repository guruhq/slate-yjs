{
  "name": "@guruhq/slate-yjs-react",
  "sideEffects": false,
  "version": "0.3.2",
  "description": "React specific components/utils for slate-yjs.",
  "keywords": [
    "slate",
    "yjs",
    "collaborative",
    "react"
  ],
  "type": "module",
  "exports": {
    "require": "./dist/index.cjs",
    "default": "./dist/index.js"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "unpkg": "dist/index.global.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "yarn run tsup && tsc --emitDeclarationOnly",
    "release": "yarn run build && npm publish",
    "dev": "yarn run tsup --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BitPhinix/slate-yjs.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^17.0.34",
    "@types/use-sync-external-store": "^0.0.3",
    "react": "^17.0.2",
    "slate-latest": "^0.82.1",
    "slate-react-latest": "^0.83.0",
    "tsup": "^6.7.0",
    "typescript": "^4.6.3",
    "yjs": "^13.5.29"
  },
  "peerDependencies": {
    "slate-latest": "^0.82.1",
    "slate-react-latest": "^0.83.0",
    "yjs": "^13.5.29"
  },
  "bugs": {
    "url": "https://github.com/BitPhinix/slate-yjs/issues"
  },
  "homepage": "https://github.com/BitPhinix/slate-yjs#readme",
  "dependencies": {
    "@guruhq/slate-yjs-core": "^0.3.2",
    "react-dom": "^18.2.0",
    "use-sync-external-store": "^1.2.0",
    "y-protocols": "^1.0.5"
  }
}
